<%- include('partials/head.ejs', {title: 'Home'}) %>
<body>
  <%- include('partials/navbar.ejs') %>
  <%- include('partials/pageHeader.ejs', {pageTitle: 'List All Course'}) %>
    <br>
    <div class="text-center">
        <div class="container">
            <div class="card">
                <div class="card body">
                    <div class="container">
                        <table class="table table-striped table-borderless table-dark">
                            <br>
                            <thead>
                                <tr>
                                    <th>Username</th>
                                    <th>Email</th>
                                    <th>Course</th>
                                    <th>Details student</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% data.forEach(el => { %>
                                    <tr>
                                        <td><%= el.username %></td>
                                        <td><%= el.email %></td>
                                        <td> 
                                         <%   el.Courses.forEach(elx=> { %>
                                            <%= elx.title  %> ,
                                      <% }) %>
                                        </td>
                                        <td>
                                          <button name="UserId" value="<%= el.id %>"><a href="/user/<%= el.id %>/detail">See Details</a></button>
                                        </td>
                                    </tr>
                                <% }) %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>