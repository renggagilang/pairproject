<%- include('partials/head.ejs', {title: 'Home'}) %>
<body>
  <%- include('partials/navbar.ejs') %>
  <%- include('partials/pageHeader.ejs', {pageTitle: 'List Category Course'}) %>
    <br>
    <div class="text-center">
        <div class="container">
            <div class="card">
                <div class="card body">
                    <div class="container">
                        <table class="table table-striped table-borderless table-dark">
                            <br>
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Release Year</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% data.forEach(el => { %>
                                    <tr>
                                        <td><%= el.title %></td>
                                        <td><%= el.formatedDate %></td>
                                        <td>
                                          <button> <a href="/category/<%= el.id %>">See Course</a></button>
                                        </td>
                                    </tr>
                                <% }) %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>